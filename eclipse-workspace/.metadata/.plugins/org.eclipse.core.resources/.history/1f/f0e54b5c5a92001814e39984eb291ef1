	static int height(BinaryTreeNode<T> root)
	{
		if(root==null)
		{	
			return 0;
		
		}
		
		
		int height=0;
		int leftheight=height(root.left);
		int rightheight=height(root.right);
		
		if(leftheight>rightheight)
			height=leftheight+1;
		else
			height=rightheight+1;
			
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
	
	
